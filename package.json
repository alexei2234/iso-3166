{
  "name": "iso-3166",
  "version": "3.1.0",
  "description": "ISO 3166 (standard for country codes and codes for their subdivisions)",
  "license": "MIT",
  "keywords": [
    "iso-3166",
    "iso",
    "3166",
    "3166-1",
    "3166-2",
    "3166-3",
    "country",
    "subdivision",
    "revision",
    "province",
    "region",
    "state",
    "territory",
    "code",
    "name",
    "information"
  ],
  "repository": "wooorm/iso-3166",
  "bugs": "https://github.com/wooorm/iso-3166/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "1.json",
  "files": [
    "index.json",
    "1-a2-to-1-a3.json",
    "1-a2-to-1-n.json",
    "1-a3-to-1-a2.json",
    "1-n-to-1-a2.json",
    "1-reserved.json",
    "1.json",
    "2.json",
    "3.json"
  ],
  "devDependencies": {
    "bail": "^1.0.0",
    "concat-stream": "^2.0.0",
    "hast-util-select": "^4.0.0",
    "hast-util-to-string": "^1.0.0",
    "hastscript": "^6.0.0",
    "mdast-util-heading-range": "^2.0.0",
    "node-fetch": "^2.0.0",
    "p-map": "^4.0.0",
    "prettier": "^2.0.0",
    "rehype-format": "^3.0.0",
    "rehype-parse": "^7.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "unified": "^9.0.0",
    "unist-builder": "^2.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "generate": "node build",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node test",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "complexity": "off",
      "guard-for-in": "off",
      "max-depth": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/prefer-number-properties": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      "./build-iso-3166-1-a2-table"
    ]
  }
}
